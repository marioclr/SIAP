		If Len(oRequest("AccountNumber").Item) > 0 Then
			aEmployeeComponent(S_CREDIT_ACCOUNT_NUMBER_EMPLOYEE) = UCase(oRequest("AccountNumber").Item)
		Else
			aEmployeeComponent(S_CREDIT_ACCOUNT_NUMBER_EMPLOYEE) = " "
		End If
	End If
	aEmployeeComponent(S_CREDIT_ACCOUNT_NUMBER_EMPLOYEE) = Left(aEmployeeComponent(S_CREDIT_ACCOUNT_NUMBER_EMPLOYEE), 255)

	If IsEmpty(aEmployeeComponent(N_CREDIT_PAYMENTS_NUMBER_EMPLOYEE)) Then
		If Len(oRequest("PaymentsNumber").Item) > 0 Then
			aEmployeeComponent(N_CREDIT_PAYMENTS_NUMBER_EMPLOYEE) = CLng(oRequest("PaymentsNumber").Item)
		Else
			aEmployeeComponent(N_CREDIT_PAYMENTS_NUMBER_EMPLOYEE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_CREDIT_PERIOD_ID_EMPLOYEE)) Then
		If Len(oRequest("PeriodID").Item) > 0 Then
			aEmployeeComponent(N_CREDIT_PERIOD_ID_EMPLOYEE) = CLng(oRequest("PeriodID").Item)
		Else
			aEmployeeComponent(N_CREDIT_PERIOD_ID_EMPLOYEE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE)) Then
		If False Then
			If Len(oRequest("ConceptEndYear").Item) > 0 Then
				aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE) = CLng(oRequest("ConceptEndYear").Item & Right(("0" & oRequest("ConceptEndMonth").Item), Len("00")) & Right(("0" & oRequest("ConceptEndDay").Item), Len("00")))
			ElseIf Len(oRequest("ConceptEndDate").Item) > 0 Then
				aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE) = CLng(oRequest("ConceptEndDate").Item)
			Else
				aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE) = 0
			End If
		End If
		If Not IsEmpty(aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE)) Then
			aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE) = aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE)
		Else
			aEmployeeComponent(L_CREDIT_FINISH_DATE_EMPLOYEE) = 30000000
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_CREDIT_PAYMENTS_COUNTER_EMPLOYEE)) Then
		If Len(oRequest("PaymentsCounter").Item) > 0 Then
			aEmployeeComponent(N_CREDIT_PAYMENTS_COUNTER_EMPLOYEE) = CLng(oRequest("PaymentsCounter").Item)
		Else
			aEmployeeComponent(N_CREDIT_PAYMENTS_COUNTER_EMPLOYEE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_CREDIT_UPLOADED_RECORD_TYPE)) Then
		If Len(oRequest("UploadedRecordType").Item) > 0 Then
			aEmployeeComponent(N_CREDIT_UPLOADED_RECORD_TYPE) = CLng(oRequest("UploadedRecordType").Item)
		Else
			aEmployeeComponent(N_CREDIT_UPLOADED_RECORD_TYPE) = -1
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_ACCOUNT_ID_EMPLOYEE)) Then
		If Len(oRequest("AccountID").Item) > 0 Then
			aEmployeeComponent(N_ACCOUNT_ID_EMPLOYEE) = CLng(oRequest("AccountID").Item)
		Else
			aEmployeeComponent(N_ACCOUNT_ID_EMPLOYEE) = -1
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_BANK_ID_EMPLOYEE)) Then
		If Len(oRequest("BankID").Item) > 0 Then
			aEmployeeComponent(N_BANK_ID_EMPLOYEE) = CLng(oRequest("BankID").Item)
		Else
			aEmployeeComponent(N_BANK_ID_EMPLOYEE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(S_ACCOUNT_NUMBER_EMPLOYEE)) Then
		If Len(CStr(oRequest("AccountNumber").Item)) > 0 Then
			aEmployeeComponent(S_ACCOUNT_NUMBER_EMPLOYEE) = CStr(oRequest("AccountNumber").Item)
		Else
			aEmployeeComponent(S_ACCOUNT_NUMBER_EMPLOYEE) = "."
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_PAYROLL_ID_EMPLOYEE)) Then
		If Len(oRequest("PayrollID").Item) > 0 Then
			aEmployeeComponent(N_PAYROLL_ID_EMPLOYEE) = CLng(oRequest("PayrollID").Item)
		Else
			aEmployeeComponent(N_PAYROLL_ID_EMPLOYEE) = -1
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_HISTORY_LIST_RECORTS)) Then
		aEmployeeComponent(N_HISTORY_LIST_RECORTS) = -1
	End If

	If IsEmpty(aEmployeeComponent(N_CALIFICATION_YEAR)) Then
		If Len(oRequest("YearID").Item) > 0 Then
			aEmployeeComponent(N_CALIFICATION_YEAR) = CLng(oRequest("YearID").Item)
		Else
			aEmployeeComponent(N_CALIFICATION_YEAR) = -1
		End If
	End If

	If IsEmpty(aEmployeeComponent(S_EMPLOYEE_GRADE)) Then
		If Len(CStr(oRequest("EmployeeGrade").Item)) > 0 Then
			aEmployeeComponent(S_EMPLOYEE_GRADE) = CStr(oRequest("EmployeeGrade").Item)
			Select Case aEmployeeComponent(S_EMPLOYEE_GRADE)
				Case "A"
					aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 40
				Case "B"
					aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 30
				Case "C"
					aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 25
				Case "D"
					aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 20
				Case "E"
					aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 15
			End Select
		Else
			aEmployeeComponent(S_EMPLOYEE_GRADE) = ""
			aEmployeeComponent(N_EMPLOYEE_GRADE_PORCENTAGE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE)) Then
		If Len(oRequest("DocumentDateYear").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE) = CLng(oRequest("DocumentDateYear").Item & Right(("0" & oRequest("DocumentDateMonth").Item), Len("00")) & Right(("0" & oRequest("DocumentDateDay").Item), Len("00")))
		ElseIf Len(oRequest("DocumentDate").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE) = CLng(oRequest("DocumentDate").Item)
		Else
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME)) Then
		If Len(oRequest("DocumentHour").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME) = CLng(Right(("0" & oRequest("DocumentHour").Item), Len("00")) & Right(("0" & oRequest("DocumentMinute").Item), Len("00")))
		ElseIf Len(oRequest("DocumentTime").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME) = CLng(oRequest("DocumentTime").Item)
		Else
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE_2)) Then
		If Len(oRequest("Document2DateYear").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE_2) = CLng(oRequest("Document2DateYear").Item & Right(("0" & oRequest("Document2DateMonth").Item), Len("00")) & Right(("0" & oRequest("Document2DateDay").Item), Len("00")))
		ElseIf Len(oRequest("Document2Date").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE_2) = CLng(oRequest("Document2Date").Item)
		Else
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_DATE_2) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME_2)) Then
		If Len(oRequest("Document2Hour").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME_2) = CLng(Right(("0" & oRequest("Document2Hour").Item), Len("00")) & Right(("0" & oRequest("Document2Minute").Item), Len("00")))
		ElseIf Len(oRequest("Document2Time").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME_2) = CLng(oRequest("Document2Time").Item)
		Else
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TIME_2) = 0
		End If
	End If

	If IsEmpty(aEmployeeComponent(S_EMPLOYEE_AUTHORIZERS)) Then
		If Len(oRequest("Authorizers").Item) > 0 Then
			aEmployeeComponent(S_EMPLOYEE_AUTHORIZERS) = Replace(oRequest("Authorizers").Item, " ", "")
		Else
			aEmployeeComponent(S_EMPLOYEE_AUTHORIZERS) = "-1"
		End If
	End If

	If IsEmpty(aEmployeeComponent(S_EMPLOYEE_AUTHORIZED)) Then
		If Len(oRequest("Authorized").Item) > 0 Then
			aEmployeeComponent(S_EMPLOYEE_AUTHORIZED) = Replace(oRequest("Authorized").Item, " ", "")
		Else
			aEmployeeComponent(S_EMPLOYEE_AUTHORIZED) = "-1"
		End If
	End If

	If IsEmpty(aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TYPE)) Then
		If Len(oRequest("DocumentTypeID").Item) > 0 Then
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TYPE) = CLng(oRequest("DocumentTypeID").Item)
		Else
			aEmployeeComponent(N_EMPLOYEE_DOCUMENT_TYPE) = 0
		End If
	End If

	aEmployeeComponent(S_EXCLUDED_CONCEPTS_ID_EMPLOYEE) = ""
	aEmployeeComponent(S_URL_EMPLOYEE) = ""
	aEmployeeComponent(S_QUERY_CONDITION_EMPLOYEE) = ""
	Call GetOptions(oRequest, oADODBConnection, aOptionsComponent, sErrorDescription)
	If Len(oRequest("SortColumn").Item) > 0 Then
		aEmployeeComponent(S_SORT_COLUMN_EMPLOYEE) = oRequest("SortColumn").Item
		Call SetOption(aOptionsComponent, EMPLOYEE_ORDER_OPTION, aEmployeeComponent(S_SORT_COLUMN_EMPLOYEE), sErrorDescription)
	Else
		aEmployeeComponent(S_SORT_COLUMN_EMPLOYEE) = GetOption(aOptionsComponent, EMPLOYEE_ORDER_OPTION)
	End If
	If Len(oRequest("Desc").Item) > 0 Then
		aEmployeeComponent(B_SORT_DESCENDING_EMPLOYEE) = (StrComp(oRequest("Desc").Item, "1", vbBinaryCompare) = 0)
		Call SetOption(aOptionsComponent, EMPLOYEE_SORT_OPTION, oRequest("Desc").Item, sErrorDescription)
	Else
		aEmployeeComponent(B_SORT_DESCENDING_EMPLOYEE) = (StrComp(GetOption(aOptionsComponent, EMPLOYEE_SORT_OPTION), "0", vbBinaryCompare) = 0)
	End If
	Call ModifyOptions(oRequest, oADODBConnection, aOptionsComponent, sErrorDescription)
	aEmployeeComponent(B_CANCEL_CONCEPT_FOR_EMPLOYEE) = False
	aEmployeeComponent(B_CHECK_FOR_DUPLICATED_EMPLOYEE) = True
	aEmployeeComponent(B_IS_DUPLICATED_EMPLOYEE) = False

	aEmployeeComponent(B_COMPONENT_INITIALIZED_EMPLOYEE) = True
	InitializeEmployeeComponent = Err.Number
	Err.Clear
End Function
%>